</head>
<body>
    <div id="divcarga" class="loader loader-default" data-text="Cargando... por favor espere" blink></div>
    <div id="divcarga2" class="loader loader-default is-active" data-text="Cargando... por favor espere" blink></div>
    @RenderBody()
    <script type="text/javascript">
        $('#divcarga2').hide();

        var vgrutaprincipal = "@Url.Content("~")";
        var datourlp = window.location.pathname;
        var rutaservidorp = window.location.host;

        function appendHtml(el, str) {
            var div = document.createElement('div');
            div.innerHTML = str;
            while (div.children.length > 0) {
                el.appendChild(div.children[0]);
            }
        }

        function redirectPost(location, nameparam, value) {
            var form = '';
            //for(args, function (key, value) {
            //    value = value.split('"').join('\"');
            //});

            var rt = '<form id="rtmvc" action="' + location + '" method="POST">'
                + '<input type="hidden" name="' + nameparam + '" value="' + value + '">'
                  + '</form>'
            var data = appendHtml(document.body, rt);
            document.getElementById("rtmvc").submit();

        }

        function redirectPostV2(location, nameparam1, value1, nameparam2, value2) {
            var form = '';

            var rt = '<form id="rtmvc" action="' + location + '" method="POST">'
                + '<input type="hidden" name="' + nameparam1 + '" value="' + value1 + '">'
                + '<input type="hidden" name="' + nameparam2 + '" value="' + value2 + '">'
                + '</form>'
            var data = appendHtml(document.body, rt);
            document.getElementById("rtmvc").submit();

        }

        function redirectPostV3(location, nameparam1, value1, nameparam2, value2, nameparam3, value3) {
            var form = '';

            var rt = '<form id="rtmvc" action="' + location + '" method="POST">'
                + '<input type="hidden" name="' + nameparam1 + '" value="' + value1 + '">'
                + '<input type="hidden" name="' + nameparam2 + '" value="' + value2 + '">'
                + '<input type="hidden" name="' + nameparam3 + '" value="' + value3 + '">'
                + '</form>'
            var data = appendHtml(document.body, rt);
            document.getElementById("rtmvc").submit();

        }

        function redirectPostV4(location, nameparam1, value1, nameparam2, value2, nameparam3, value3, nameparam4, value4, nameparam5, value5) {
            var form = '';

            var rt = '<form id="rtmvc" action="' + location + '" method="POST">'
                + '<input type="hidden" name="' + nameparam1 + '" value="' + value1 + '">'
                + '<input type="hidden" name="' + nameparam2 + '" value="' + value2 + '">'
                + '<input type="hidden" name="' + nameparam3 + '" value="' + value3 + '">'
                + '<input type="hidden" name="' + nameparam4 + '" value="' + value4 + '">'
                + '<input type="hidden" name="' + nameparam5 + '" value="' + value5 + '">'
                + '</form>'
            var data = appendHtml(document.body, rt);
            document.getElementById("rtmvc").submit();

        }

        function CerrarOV() {
            $.ajax({
                url: vgrutaprincipal + "actividad/CerrarOV",
                cache: false,
                success: function () {
                    window.location.href = vgrutaprincipal + "actividad/registro";
                },
                error: function () {
                    console.log("No se ha podido obtener la información");
                }
            });
        }

    </script>
    <script src="~/Content/lib/vue/js/vue.js"></script>
    <script src="~/Content/lib/vuetify/js/vuetify.js"></script>
    <script src="~/Content/lib/axios/js/axios.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
